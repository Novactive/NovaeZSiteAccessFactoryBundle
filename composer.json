{
    "name": "novactive/ezsiteaccessfactorybundle",
    "description": "Ibexa bundle that will allow you to create SiteAccess (website) on \nthe fly on top of an existing DBX instance",
    "keywords": [
        "siteaccess",
        "ibexa",
        "novactive",
        "websitefactory"
    ],
    "homepage": "https://github.com/Novactive/NovaeZSiteAccessFactoryBundle",
    "type": "ibexa-bundle",
    "authors": [
        {
            "name": "Novactive",
            "homepage": "https://www.novactive.us",
            "email": "dir.tech@novactive.com"
        },
        {
            "name": "Sebastien Morel",
            "homepage": "https://github.com/Plopix",
            "email": "morel.seb@gmail.com"
        }
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*",
        "novactive/ezextrabundle": "dev-ez-extra-ibexa-4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpmd/phpmd": "^2.7",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Novactive\\Bundle\\eZSiteAccessFactoryBundle\\": "bundle",
            "Novactive\\Bundle\\eZSiteAccessFactoryBundle\\Tests\\": "tests/Tests"
        }
    },
    "extra": {
        "thanks": {
            "name": "ibexa/core",
            "url": "https://github.com/ibexa/core"
        }
    }
}
