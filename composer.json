{
    "name": "novactive/ezsiteaccessfactorybundle",
    "description": "eZPlatform bundle that will allow you to create SiteAccess (website) on \nthe fly on top of an existing eZ Platform instance",
    "keywords": [
        "siteaccess",
        "ezplatform",
        "novactive",
        "websitefactory"
    ],
    "homepage": "https://github.com/Novactive/NovaeZSiteAccessFactoryBundle",
    "type": "ezplatform-bundle",
    "authors": [
        {
            "name": "Novactive",
            "homepage": "https://www.novactive.us",
            "email": "dir.tech@novactive.com"
        },
        {
            "name": "Sebastien Morel",
            "homepage": "https://github.com/Plopix",
            "email": "morel.seb@gmail.com"
        }
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ezsystems/ezpublish-kernel": "^7.4",
        "novactive/ezextrabundle": "^2.0",
        "kaliop/ezmigrationbundle": "^5.10"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpmd/phpmd": "^2.7",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Novactive\\Bundle\\eZSiteAccessFactoryBundle\\": "bundle",
            "Novactive\\Bundle\\eZSiteAccessFactoryBundle\\Tests\\": "tests/Tests"
        }
    },
    "extra": {
        "thanks": {
            "name": "ezsystems/ezplatform",
            "url": "https://github.com/ezsystems/ezplatform"
        }
    }
}
